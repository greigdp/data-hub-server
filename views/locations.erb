<table>
  <tr>
    <th>user id</th>
    <th>timestamp</th>
    <th>provider</th>
    <th>latitude</th>
    <th>longitude</th>
    <th>accuracy</th>
    <th>created at</th>
  </tr>
  <% @locations.each do |location| %>
    <tr>
      <td><%= location.user_id %></td>
      <td><%= location.timestamp %></td>
      <td><%= location.provider %></td>
      <td><%= location.latitude %></td>
      <td><%= location.longitude %></td>
      <td><%= location.accuracy %></td>
      <td><%= location.created_at %></td>
    </tr>
  <% end %>
</table>

